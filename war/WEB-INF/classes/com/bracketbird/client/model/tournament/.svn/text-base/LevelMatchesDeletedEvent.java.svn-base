package com.moveit.client.model.tournament;


/**
 *
 */
public class LevelMatchesDeletedEvent extends TournamentEvent {

    private TournamentLevel level;

    public LevelMatchesDeletedEvent(TournamentLevel level, boolean fromClient) {
        super(fromClient);
        this.level = level;
    }

    public TournamentLevel getLevel() {
        return level;
    }

    public void setLevel(TournamentLevel level) {
        this.level = level;
    }
}